@startuml

title __SRC's Class Diagram__\n

abstract class Canard {
    {abstract} + afficher()
    + cancaner()
    + nager()
    + voler()
}


class CanardEnPlastique {
    + CanardEnPlastique()
    + afficher()
}


class Cancan {
    + cancaner()
}


interface CancanStrategie {
    {abstract} + cancaner()
}


class Colvert {
    + Colvert()
    + afficher()
}


class NoCancan {
    + cancaner()
}


class NoVol {
    + voler()
}


class Vol {
    + voler()
}


interface VolStrategie {
    {abstract} + voler()
}


Canard o-- CancanStrategie : cancanerImpl
Canard o-- VolStrategie : volerImpl
CanardEnPlastique -up-|> Canard
Cancan .up.|> CancanStrategie
Colvert -up-|> Canard
NoCancan .up.|> CancanStrategie
NoVol .up.|> VolStrategie
Vol .up.|> VolStrategie


